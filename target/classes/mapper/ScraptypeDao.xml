<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cbb.wasteRecovery.dao.ScraptypeDao">
<resultMap type="org.cbb.wasteRecovery.bean.Scraptype" id="type">
  <id property="id" column="id"/>
  <result property="typeName" column="typeName"/>
  <result property="avergePriceFU" column="avergePriceFU"/>
  <result property="avergePriceFC" column="avergePriceFC"/>



</resultMap>
    <select id="selectByid" resultType="org.cbb.wasteRecovery.bean.Scraptype" parameterType="int">
        select id,typeName,avergePriceFU,avergePriceFC
        from scrap
        where
           id=#{id}
       
        
    </select>

</mapper>