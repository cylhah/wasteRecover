<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cbb.wasteRecovery.dao.BankAccountDao">


    <insert id="insertBankAccount">

        insert  into bankaccount(id,account,bank_code,username)
        values (#{id},#{account},#{bank_code},#{username})
    </insert>


    <delete id="deleteBankAccount">
        delete
        from
       bankaccount
        where
        id=#{id}

    </delete>
</mapper>